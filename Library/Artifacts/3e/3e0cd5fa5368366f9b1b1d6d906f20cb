            TestUtils.AreEqual(r1, a1 & b1);

            int4 a2 = int4(327392481, 2143619546, 1873208293, 126982769);
            int b2 = (1619794917);
            int4 r2 = int4(8394977, 1619265984, 1619264485, 8395361);
